{
  "swagger": "2.0",
  "info": {
    "title": "DOG API",
    "version": "1.0.0",
    "description": "API of 왈왈왈"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "tags": [],
  "schemes": [
    "http"
  ],
  "securityDefinitions": {
    "bearerAuth": {
      "type": "apiKey",
      "name": "Authorization",
      "scheme": "bearer",
      "in": "header",
      "bearerFormat": "JWT"
    }
  },
  "consumes": [],
  "produces": [],
  "paths": {
    "/users/checkDup": {
      "post": {
        "tags": ["USERS"],
        "description": "userEmail 중복 검사",
        "parameters": [
          {
            "name": "userEmail",
            "in": "query",
            "type": "string",
            "description": "Check tour email",
            "required":true
          }
        ],
        "responses": {
          "200":{
            "description": "사용 가능한 EMAIL입니다."
          },
          "400": {
            "description": "EMAIL 확인 또는 중복입니다."
          }
        }
      }
    },

    "/users/signUp": {
      "post": {
        "tags": ["USERS"],
        "description": "회원가입",
        "consumes": "multipart/form-data",
        "produces": "application/json",
        "parameters": [
          {
            "name": "userImage",
            "in": "formData",
            "type": "file",
            "description": "userImage to upload"
          },
          {
            "name": "userEmail",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "password",
            "in": "formData",
            "required": true,
            "type": "string"
          }, 
          {
            "name": "confirmPassword",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "userNickname",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "userGender",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "userAge",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "userLocation",
            "in": "formData",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201":{
            "description": "회원가입이 완료되었습니다."
          },
          "401": {
            "description": "사용중인 닉네임입니다."
          },
          "402": {
            "description": "이메일 형식이 올바르지 않습니다."
          },
          "403": {
            "description": "비밀번호와 비밀번호확인을 동일하게 입력해주세요."
          },
          "404": {
            "description": "비밀번호는 3자리 이상으로 작성해주세요."
          },
          "405": {
            "description": "비밀번호에 아이디가 포함되어있습니다."
          }
        }
      }
    },
    "/users/login": {
      "post": {
        "tags": ["USERS"],
        "description": "로그인",
        "parameters": [
          {
            "name": "userId",
            "in": "query",
            "required": true,
            "type": "string",
            "description":"The user email for login"
          },
          {
            "name": "password",
            "in": "query",
            "required": true,
            "type": "string",
            "description":"The user password for login"
          }
        ],
        "responses": {
          "200":{
            "description": "JWT token"
          },
          "400": {
            "description": "이메일 또는 패스워드가 잘 못 되었습니다."
          }
        }
      }
    },
    "/dogs/dogInfo": {
      "post": {
        "description": "로그인 한 유저가 강아지를 등록하는 API",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": [
          "DOGS"
        ],
        "name": "강아지 등록",
        "summary": "로그인한 유저의 강아지 정보를 추가한다.",
        "consumes": "multipart/form-data",
        "produces": "application/json",
        "parameters": [
          {
            "name": "dogImage",
            "in": "formData",
            "type": "file",
            "description": "dog_image"
          },
          {
            "name": "req.body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "dogGender": {
                  "type": "String",
                  "description": "강아지 성별"
                },
                "dogName": {
                  "type": "String",
                  "description": "강아지 이름"
                },
                "dogSize": {
                  "type": "String",
                  "description": "강아지 크기"
                },
                "dogBreed": {
                  "type": "String",
                  "description": "강아지 종"
                },
                "dogAge": {
                  "type": "String",
                  "description": "강아지 나이"
                },
                "neutral": {
                  "type": "String",
                  "description": "중성화 여부"
                },
                "dogComment": {
                  "type": "String",
                  "description": "강아지에 대한 한 줄 소개"
                }
              },
              "example": {
                "dogGender": "남",
                "dogName": "가을이",
                "dogSize": "중형견",
                "dogBreed": "비숑",
                "dogAge": "8세 이상",
                "neutral": "true",
                "dogComment": "짖지 않아요"
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "db에 강아지 등록 성공"
          },
          "400": {
            "description": "db에 저장하기 실패"
          },
          "500": {
            "description": "로그인이 필요합니다."
          }
        }
      }
    },
    "/dogs/": {
      "get": {
        "description": "로그인 한 유저의 강아지를 조회하는 API",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": [
          "DOGS"
        ],
        "name": "강아지 정보 조회",
        "summary": "로그인한 유저의 강아지 정보를 조회",
        "parameters": [],
        "responses": {
          "200": {
            "description": "success: true"
          },
          "400": {
            "description": "success: false"
          },
          "500": {
            "description": "로그인이 필요합니다."
          }
        }
      },
      "put": {
        "description": "로그인한 유저의 강아지 정보를 수정하는 API",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": [
          "DOGS"
        ],
        "name": "강아지 정보 수정",
        "summary": "로그인한 유저의 강아지 정보를 수정",
        "parameters": [
          {
            "name": "req.body",
            "in": "formData",
            "required": true,
            "type": "file",
            "description": "dog_image"
          },
          {
            "name": "req.body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "dogGender": {
                  "type": "String",
                  "description": "강아지 성별"
                },
                "dogName": {
                  "type": "String",
                  "description": "강아지 이름"
                },
                "dogSize": {
                  "type": "String",
                  "description": "강아지 크기"
                },
                "dogBreed": {
                  "type": "String",
                  "description": "강아지 종"
                },
                "dogAge": {
                  "type": "String",
                  "description": "강아지 나이"
                },
                "neutral": {
                  "type": "String",
                  "description": "중성화 여부"
                },
                "dogComment": {
                  "type": "String",
                  "description": "강아지에 대한 한 줄 소개"
                }
              },
              "example": {
                "dogGender": "남",
                "dogName": "가을이",
                "dogSize": "중형견",
                "dogBreed": "비숑",
                "dogAge": "8세 이상",
                "neutral": "true",
                "dogComment": "짖지 않아요"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "수정이 완료되었습니다."
          },
          "400": {
            "description": "수정에 실패하였습니다."
          },
          "500": {
            "description": "로그인이 필요합니다."
          }
        }
      }
    }
  },
  "definitions": {}
}