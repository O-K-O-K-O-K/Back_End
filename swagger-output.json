              "type": "object",
              "properties": {
                "meetingDate": {
                  "type": "String",
                  "description": "약속날짜, 시간"
                },
                "wishDesc": {
                  "type": "String",
                  "description": "코멘트"
                },
                "locationCategory": {
                  "type": "String",
