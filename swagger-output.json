{
  "swagger": "2.0",
  "info": {
    "title": "DOG API",
    "version": "1.0.0",
    "description": "API of 왈왈왈"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "tags": [],
  "schemes": [
    "http"
  ],
  "securityDefinitions": {
    "bearerAuth": {
      "type": "apiKey",
      "name": "Authorization",
      "scheme": "bearer",
      "in": "header",
      "bearerFormat": "JWT"
    }
  },
  "consumes": [],
  "produces": [],
  "paths": {
    "/users/signUp": {
      "post": {
        "tags": ["USERS"],
        "description": "회원가입",
        "consumes": "multipart/form-data",
        "produces": "application/json",
        "parameters": [
          {
            "name": "userImage",
            "in": "formData",
            "type": "file",
            "description": "userImage to upload"
          },
          {
            "name": "userEmail",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "password",
            "in": "formData",
            "required": true,
            "type": "string"
          }, 
          {
            "name": "confirmPassword",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "userNickname",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "userGender",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "userAge",
            "in": "formData",
            "required": true,
            "type": "string"
          },
          {
            "name": "userLocation",
            "in": "formData",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201":{
            "description": "회원가입이 완료되었습니다."
          },
          "401": {
            "description": "사용중인 닉네임입니다."
          },
          "402": {
            "description": "이메일 형식이 올바르지 않습니다."
          },
          "403": {
            "description": "비밀번호와 비밀번호확인을 동일하게 입력해주세요."
          },
          "404": {
            "description": "비밀번호는 3자리 이상으로 작성해주세요."
          },
          "405": {
            "description": "비밀번호에 아이디가 포함되어있습니다."
          }
        }
      }
    },
    
    "/users/login": {
      "post": {
        "tags": ["USERS"],
        "description": "로그인",
        "parameters": [
          {
            "name": "userId",
            "in": "query",
            "required": true,
            "type": "string",
            "description":"The user email for login"
          },
          {
            "name": "password",
            "in": "query",
            "required": true,
            "type": "string",
            "description":"The user password for login"
          }
        ],
        "responses": {
          "200":{
            "description": "JWT token"
          },
          "400": {
            "description": "이메일 또는 패스워드가 잘 못 되었습니다."
          }
        }
        }
      },

    "/users/checkDup": {
      "post": {
        "tags": ["USERS"],
        "description": "userEmail 중복 검사",
        "parameters": [
          {
            "name": "userEmail",
            "in": "query",
            "type": "string",
            "description": "Check tour email",
            "required":true
          }
        ],
        "responses": {
          "200":{
            "description": "사용 가능한 EMAIL입니다."
          },
          "400": {
            "description": "EMAIL 확인 또는 중복입니다."
          }
        }
      }
    },
    "/posts/write": {
      "post": {
        "description": "산책 약속 등록하는 API",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": [
          "Detail"
        ],
        "name": "산책 약속 등록",
        "summary": "산책 약속 등록.",
        "parameters": [
          {
            "name": "req.body",
            "in": "req.body"
          },
          {
            "name": "req.body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "meetingDate": {
                  "type": "String",
                  "description": "약속날짜, 시간"
                },
                "wishDesc": {
                  "type": "String",
                  "description": "코멘트"
                },
                "locationCategory": {
                  "type": "String",
                  "description": "장소카테고리"
                },
                "dogCount": {
                  "type": "Number",
                  "description": "산책할 강아지 수"
                },
                "TotalTime": {
                  "type": "String",
                  "description": "산책 시간"
                },
                "startLocationAddress": {
                  "type": "String",
                  "description": "산책 시작 장소 "
                },
                "endLocationAddress": {
                  "type": "String",
                  "description": "산책 종료 장소 "
                },
                "totalDistance": {
                  "type": "String",
                  "description": "총 산책 길이 "
                },
                "routeColor": {
                  "type": "String",
                  "description": "산책 종료 장소 "
                },
                "routeName": {
                  "type": "String",
                  "description": "산책 경로 이름 "
                }
              },
              "example": {
                "meetingDate": "2021-12-20T18:05:00.000Z",
                "wishDesc": "소형견 산책 가능",
                "locationCategory": "올림픽 공원",
                "dogCount": 2,
                "totalTime" : "2시간",
                "startLocationAddress": "서울 성동구1동",
                "endLocationAddress": "서울 성동구1동",
                "totalDistance" : "2km",
                "routeColor": "green",
                "routeName": "1"
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "게시글이 성공적으로 포스팅 되었습니다!"
          },
          "400": {
            "description": "400 에러 게시중 오류가 발생 하였습니다!"
          },
          "500": {
            "description": "500 에러 게시중 오류가 발생 하였습니다!."
          }
        }
      }
    },
    "/posts/:postId": {
      "get": {
        "description": "산책약속 상세 조회하기",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": [
          "Detail"
        ],
        "name": "산책 약속 상세 조회하기",
        "summary": "산책 약속 상세 조회하기",
        "parameters": [
          {
            "name": "postId",
            "in": "path",
            "required": true,
            "default": 1,
            "description": "500 에러 조회중 오류가 발생 하였습니다!."
          }
        ],
        "responses": {
          "200": {
            "description": "success: true"
          },
          "400": {
            "description": "success: false"
          },
          "500": {
            "description": "로그인이 필요합니다."
          }
        }
      },
      "put": {
        "description": "로그인한 유저의 산책약속을 수정하는 API",
