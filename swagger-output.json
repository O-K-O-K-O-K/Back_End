                },
                "dogCount": {
                  "type": "Number",
                  "description": "산책할 강아지 수"
                },
                "TotalTime": {
                  "type": "String",
                  "description": "산책 시간"
                },
                "startLocationAddress": {
                  "type": "String",
                  "description": "산책 시작 장소 "
                },
                "endLocationAddress": {
                  "type": "String",
                  "description": "산책 종료 장소 "
                },
                "totalDistance": {
                  "type": "String",
                  "description": "총 산책 길이 "
                },
                "routeColor": {
                  "type": "String",
                  "description": "산책 종료 장소 "
                },
                "routeName": {
